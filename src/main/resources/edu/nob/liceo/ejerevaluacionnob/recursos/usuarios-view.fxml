<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="417.0" prefWidth="672.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.nob.liceo.ejerevaluacionnob.controllers.UsuarioController">
<top>
    <HBox alignment="CENTER_LEFT" prefHeight="50.0" spacing="10.0" BorderPane.alignment="CENTER_LEFT">
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </padding>
        <Label text="Buscar Usuarios">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Label>
        <TextField fx:id="tfBuscar" prefWidth="300.0" promptText="Buscar jugadores" />
    </HBox>
</top>
<center>
    <TableView fx:id="tablaJugadores" prefHeight="289.0" prefWidth="524.0" BorderPane.alignment="CENTER">
        <columns>
            <TableColumn fx:id="colId" prefWidth="50.0" text="Id" />
            <TableColumn fx:id="colNombre" prefWidth="150.0" text="Nombre" />
            <TableColumn fx:id="colApellido" prefWidth="150.0" text="Apellido" />
            <TableColumn fx:id="colNickname" prefWidth="150.0" text="Nickname" />
            <TableColumn fx:id="colFechaNacimiento" prefWidth="150.0" text="Fecha Nac." />
        </columns>
        <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </BorderPane.margin>
    </TableView>
</center>
</BorderPane>
